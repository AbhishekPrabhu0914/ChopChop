{
  "version": 2,
  "builds": [
    {
      "src": "nova-chat-frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/nova-chat-frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_anon_key",
    "PYTHON_BACKEND_URL": "@python_backend_url"
  }
}
